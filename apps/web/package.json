    },
    "devDependencies": {
        "@types/node": "^18.11.10",
        "@types/react": "^18.0.25",
        "@types/react-dom": "^18.0.9",
        "autoprefixer": "^10.4.13",
        "eslint-config-custom": "*",
        "postcss": "^8.4.19",
        "tailwindcss": "^3.2.4",
        "typescript": "^4.9.3",
        "tsconfig": "*",
        "database": "*",
        "tailwind-config": "*"
    }
}
